<launch>

<rosparam file="$(find ROBOT-URDF_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="ROBOT-URDF" args="旋转 1_position_controller 旋转 2_position_controller 旋转 3_position_controller 旋转 4_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/ROBOT-URDF/joint_states"/>
</node>

</launch>